import java.util.Scanner;

public class TimesTable {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int row = sc.nextInt();
		
		for(int i = 1; i <= 9; i++) {
			for(int j = 1; j <= 9; j++) {
				System.out.printf("%d x %d = %d",i, j, i*j);
			}
		}
		
		
		
	}

}


